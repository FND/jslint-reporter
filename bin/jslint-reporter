#!/usr/bin/env node

var jslr = require("../wrapper");

// capture STDOUT
var _stdout = process.stdout.write;
process.stdout.write = function(string, encoding, fd) {
	_stdout(typeof string);
	if(string) {
		_stdout(string);
		// TODO: exit 1
	} else { // XXX: never triggered?
		// TODO: exit 0
	}
};

process.argv.splice(2, 0, "--goodparts");
jslr.main(process.argv);
